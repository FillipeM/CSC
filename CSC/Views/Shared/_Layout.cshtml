<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CSC</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="nav navbar-dark bg-dark navbar-expand-lg">
            <a class="navbar-brand" href="#">CSC</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/Home/Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/AberturaChamado/Index">Abertura Chamado</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/Categoria/Index">Categorias</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/Departamento/Index">Departamentos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/Diretoria/Index">Diretorias</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/NivelAtendimento/Index">Níveis de Atendimento</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/Situacao/Index">Situações</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://10.6.0.8:94/TipoAtendimento/Index">Tipos de Atendimentos</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>

    <div class="col-lg-12 full-height" >
        <main role="main" class="pb-3 full-height">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
